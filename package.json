{
  "name": "@tscircuit/run-snippet",
  "version": "0.0.1",
  "module": "dist/index.js",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node",
    "typecheck": "bun x tsc --noEmit",
    "format": "bun x @biomejs/biome check --apply src",
    "format:check": "bun x @biomejs/biome check src"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@types/bun": "latest",
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@tscircuit/eval-webworker": "^0.0.20"
  }
}
